{
  "name": "porosearch-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "author": "Matheus Henrique matheus.181137@gmail.com",
  "description": "",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --fix",
    "format": "prettier --check ./src",
    "prepare": "husky install",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.7",
    "@tamagui/animations-react-native": "^1.46.2",
    "@tamagui/babel-plugin": "^1.46.2",
    "@tamagui/config": "^1.46.2",
    "@tamagui/font-inter": "^1.46.2",
    "@tamagui/react-native-media-driver": "^1.46.2",
    "@tamagui/theme-base": "^1.46.2",
    "@types/react": "~18.2.14",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "expo": "^49.0.3",
    "expo-font": "^11.4.0",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.3",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "^0.19.7",
    "tamagui": "^1.46.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "bootstrap",
    "react",
    "react native",
    "expo",
    "jest"
  ]
}
